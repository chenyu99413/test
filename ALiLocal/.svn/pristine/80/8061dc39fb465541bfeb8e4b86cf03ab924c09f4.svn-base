ALTER TABLE `tb_routes` ADD `network_code` VARCHAR(8) NOT NULL DEFAULT 'UPS' AFTER `tracking_no`;
drop view v_route_latest;
create view v_route_latest as select `o`.`order_id` AS `order_id`,`o`.`ali_order_no` AS `ali_order_no`,`o`.`order_status` AS `order_status`,`o`.`consignee_country_code` AS `consignee_country_code`,`o`.`warehouse_out_time` AS `warehouse_out_time`,`o`.`channel_id` AS `channel_id`,`o`.`tracking_no` AS `tracking_no`,`rot2`.`location` AS `location`,`rot2`.`time` AS `time`,`rot2`.`description` AS `description`,`rot2`.`create_time` AS `create_time`,`tr`.`confirm_flag` AS `confirm_flag` from (((`ali1688`.`tb_order` `o` left join `ali1688`.`v_route_latest_ids` `rot` on((convert(`o`.`tracking_no` using utf8mb4) = `rot`.`tracking_no`))) left join `ali1688`.`tb_routes` `rot2` on(((`rot2`.`id` = `rot`.`latest`) and (`rot2`.`tracking_no` = `rot`.`tracking_no`)))) left join `ali1688`.`tb_tracking` `tr` on((`tr`.`route_id` = `rot2`.`id`))) where ((`o`.`order_status` in (7,8)) or ((`o`.`order_status` = 12) and (`o`.`order_status_copy` in (7,8))));
CREATE TABLE `tb_code_warehouse` (
	`id` int(11) NOT NULL AUTO_INCREMENT,
	`warehouse` VARCHAR(30) COLLATE utf8_general_ci DEFAULT NULL COMMENT '仓库代码',
	`department_id` int(11) DEFAULT NULL COMMENT '部门ID',
	`department_name` VARCHAR(10) COLLATE utf8_general_ci DEFAULT NULL COMMENT '部门名称',
	`create_time` int(11) DEFAULT NULL,
	`update_time` int(11) DEFAULT NULL,
	 PRIMARY KEY (`id`)
)ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='仓库代码表' AUTO_INCREMENT=1 ;
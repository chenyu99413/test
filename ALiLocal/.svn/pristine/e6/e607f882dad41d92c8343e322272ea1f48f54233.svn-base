CREATE TABLE `tb_staff_edit_log` (
`id`  int(10) NOT NULL AUTO_INCREMENT ,
`edit_staff_id`  int(11) NULL DEFAULT NULL COMMENT '修改权限的对象' ,
`edit_staff_name`  varchar(100) NULL DEFAULT NULL COMMENT '修改权限的对象' ,
`edit_contect`  varchar(200) NULL DEFAULT NULL COMMENT '修改内容' ,
`edit_time`  int(11) NULL DEFAULT NULL COMMENT '修改时间' ,
`operator_name`  varchar(100) NULL DEFAULT NULL COMMENT '操作人员' ,
`create_time`  int(11) NULL DEFAULT NULL COMMENT '创建时间' ,
`update_time`  int(11) NULL DEFAULT NULL COMMENT '修改时间' ,
PRIMARY KEY (`id`)
) ENGINE=InnoDB CHARSET=utf8 COMMENT='角色修改日志';
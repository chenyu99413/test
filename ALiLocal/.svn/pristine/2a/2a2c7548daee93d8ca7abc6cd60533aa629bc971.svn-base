				}
				if(account=='4F1R24'){
					for(var s=0;s<2;s++){
						if(s==0){
							AF.func("Build", "<?php echo $_BASE_DIR?>public/supcan/upsprintsheet3.xml");
							AF.func("SetSource", "ds1 \r\n "+'<?php echo $_BASE_DIR?>_tmp/upload/'+data.tracking_no+'.json');
							switchPrinter('P1606dn');

			                  $tw=(floor($package->length_out)*floor($package->width_out)*floor($package->height_out))/$ratio;
			              }?>
                   <?php if($order->packing_type=='PAK'):?>
                   <?php $total_weight=$package->weight_out;
                         $total_weight=sprintf("%.2f",$total_weight);?>
                   <?php else :?>
                    <?php 
    				      if($tw>$package->weight_out){
			                  $weight_cost_out=ceil($tw/0.5)*0.5;
			              }else{
			                  $weight_cost_out=ceil($package->weight_out/0.5)*0.5;
			              }
			              $total_weight=$weight_cost_out;
				    ?>
				   <?php endif;?>
				   <?php if($order->service_code=='US-FY'){
				       $total_weight='';
				       $total_weight = $package->weight_out;
            	       $total_weight=sprintf("%.2f",$total_weight);
				   }?>
				   <?php $twq += $total_weight*$package->quantity_out;?>
                   <td style="text-align: center"><?php echo sprintf("%.2f",$tw)?></td>
                   <td style="text-align: center"><?php echo sprintf("%.2f",$total_weight)?></td>
                   <td style="text-align: center"><?php echo sprintf("%.2f",$total_weight*$package->quantity_out)?></td>
                   <?php if ($order->order_status=='4' || $order->order_status=='6'):?>
                   <td><button class="btn btn-mini btn-danger" href="javascript:void(0)" onclick="removed(this)"><i class="icon-remove"></i></button></td>
                   <?php endif;?>
               </tr>
               <?php endforeach;?>
               <?php if ($order->order_status=='4' || $order->order_status=='6'):?>
               <tr>
                    <td colspan="6">
                        <div style="line-height:25px;">
                        	<a class="btn btn-mini btn-primary" href="javascript:void(0)" onclick="save(this)" data="out"
                        		style="margin-right: 10px;">
                        		<i class="icon-save"></i>
                        		提交数据
                        	</a>
                    	</div>
                    </td>
                    <?php if ($twq):?>
                    <td>总计：</td>
                    <td style="text-align: center;"><?php echo ($twq>20 && $order->service_code=='Express_Standard_Global') ? ceil($twq):sprintf("%.2f",$twq)?></td>
                    <?php endif;?>
               </tr>
               <?php else :?>
                    <?php if ($twq):?>
                    <td colspan="6"></td>
                    <td>总计：</td>
                    <td style="text-align: center;"><?php echo ($twq>20 && $order->service_code=='Express_Standard_Global') ? ceil($twq):sprintf("%.2f",$twq)?></td>
                    <?php endif;?>
               <?php endif;?>
           </tbody>
        </table>
     </div>
     </div>
</div>
<input type="hidden" id="package_quantity" value="<?php echo $package_quantity;?>">

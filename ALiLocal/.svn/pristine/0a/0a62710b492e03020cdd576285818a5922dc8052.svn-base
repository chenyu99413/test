CREATE TABLE `tb_total_list` (
  `total_list_id` int(11) NOT NULL AUTO_INCREMENT,
  `total_list_no` varchar(20) DEFAULT NULL COMMENT '总单号',
  `channel_group_id` varchar(11) DEFAULT NULL COMMENT '渠道分组id',
  `country_code` varchar(32) DEFAULT NULL COMMENT '国家二字码',
  `record_order_date` int(11) DEFAULT NULL COMMENT '发件日',
  `create_time` int(11) DEFAULT NULL,
  `update_time` int(11) DEFAULT NULL,
  PRIMARY KEY (`total_list_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='总单表';
